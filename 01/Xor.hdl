/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not (in=a, out=NotA);
    Not (in=b, out=NotB);
    And (a=NotA, b=b, out=AndNotA);
    And (a=a, b=NotB, out=AndNotB);
    Or (a=AndNotA, b=AndNotB, out=out);
}
